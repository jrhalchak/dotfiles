{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bdiagram",
      "camelcase": "bdiagram",
      "title": "Neorg Box Diagrams",
      "scope": "source.bdiagram",
      "file-types": [
        "norg"
      ],
      "injection-regex": "^bdiagram$",
      "class-name": "TreeSitterBdiagram"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A syntax for using ASCII characters to create box diagrams, which can be concealed and modified",
    "authors": [
      {
        "name": "JR Halchak",
        "email": "jonathan@halchak.email",
        "url": "https://www.github.com/jrhalchak"
      }
    ],
    "links": {
      "repository": "https://www.github.com/jrhalchak/bdiagram"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}